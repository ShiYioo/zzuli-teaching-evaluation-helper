<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <!-- 控制台输出 -->
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%gray([%level]) %msg%n</pattern>
        </encoder>
    </appender>

    <!-- 根日志级别设为 DEBUG -->
    <root level="DEBUG">
        <appender-ref ref="CONSOLE" />
    </root>

    <!-- 应用程序日志级别 -->
    <logger name="AutoEvaluation" level="DEBUG" />

    <!-- 第三方库日志级别 -->
    <logger name="org.java_websocket" level="INFO" />
</configuration>

